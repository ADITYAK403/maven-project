pipeline
{
agent any
stages
{
    stage('scm checkout') 
    {
    steps { git branch: 'master', url: 'https://github.com/prakashk0301/maven-project' }
    }

    stage ('code build')
    {
        steps { withMaven(jdk: 'Local_JDK', maven: 'local_maven_3.5') {
    sh 'mvn package'
    } }

    }
}

}